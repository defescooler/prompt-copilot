{
    "name": "prompto-chrome-extension",
    "version": "1.0.0",
    "description": "Prompto Chrome Extension - React, TypeScript, Vite, Tailwind, Testing, Linting",
    "author": "Your Name",
    "license": "MIT",
    "main": "src/index.tsx",
    "scripts": {
      "dev": "vite",
      "build": "vite build",
      "build:extension": "vite build --config vite.config.ts",
      "preview": "vite preview",
      "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
      "format": "prettier --write .",
      "test": "vitest run",
      "test:ui": "vitest --ui"
    },
    "dependencies": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "devDependencies": {
      "@types/react": "^18.2.37",
      "@types/react-dom": "^18.2.15",
      "@vitejs/plugin-react": "^4.2.1",
      "autoprefixer": "^10.4.16",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-react": "^7.33.2",
      "postcss": "^8.4.35",
      "prettier": "^3.2.5",
      "tailwindcss": "^3.4.3",
      "typescript": "^5.4.5",
      "vite": "^4.5.2",
      "vitest": "^1.4.0"
    },
    "browserslist": [
      "last 2 Chrome versions"
    ]
  }
  
  